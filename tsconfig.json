{
    "compilerOptions": {
      "baseUrl": "./src",
      "target": "ES6",
      "module": "ESNext",
      "moduleResolution": "node",
      "lib":[
        "DOM",
        "ESNext"
       ],
      "jsx":"react-jsx",
      "noEmit": true,
      "isolatedModules": true,
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames":true,
      "resolveJsonModule": true,
      "skipLibCheck":true
    },
    "include": ["src/**/*"]
  }